{
	"name": "amaneko",
	"version": "0.0.1",
	"author": "killbasa <contact@shaneb.xyz>",
	"license": "MIT",
	"main": "./dist/Amaneko.js",
	"private": true,
	"type": "module",
	"repository": {
		"type": "git",
		"url": "https://github.com/killbasa/amaneko.git"
	},
	"homepage": "https://github.com/killbasa/amaneko",
	"engines": {
		"node": ">=20.x"
	},
	"scripts": {
		"prepare": "husky install",
		"build": "tsup",
		"typecheck": "tsc -b src",
		"dev": "tsup --watch --onSuccess \"yarn start\"",
		"start": "node --preserve-symlinks --enable-source-maps ./dist/Amaneko.js",
		"lint": "eslint '{src,tests,docs}/*.ts'",
		"lint:fix": "eslint --fix '{src,tests,docs}/*.ts'",
		"format": "prettier --check .",
		"format:fix": "prettier --write --log-level=warn .",
		"db:generate": "prisma generate",
		"db:push": "prisma db push",
		"test": "vitest --coverage run",
		"test:watch": "vitest -w",
		"docs:dev": "vitepress dev docs",
		"docs:build": "vitepress build docs",
		"docs:preview": "vitepress preview docs",
		"docker": "docker buildx build -t killbasa/amaneko .",
		"dry-run": "yarn typecheck && yarn format && yarn lint && yarn build && yarn test",
		"update": "yarn upgrade-interactive"
	},
	"dependencies": {
		"@killbasa/redis-utils": "^0.0.5",
		"@prisma/client": "^5.3.1",
		"@sapphire/decorators": "^6.0.2",
		"@sapphire/discord.js-utilities": "^7.0.2",
		"@sapphire/duration": "^1.1.0",
		"@sapphire/fetch": "^2.4.1",
		"@sapphire/framework": "^4.6.0",
		"@sapphire/pieces": "^3.7.0",
		"@sapphire/plugin-api": "^5.1.1",
		"@sapphire/plugin-logger": "^3.0.6",
		"@sapphire/plugin-scheduled-tasks": "^7.1.2",
		"@sapphire/plugin-subcommands": "^4.2.1",
		"bullmq": "^4.12.0",
		"discord.js": "^14.13.0",
		"dotenv": "^16.3.1",
		"googleapis": "^126.0.1",
		"humanize-duration": "^3.30.0",
		"ioredis": "^5.3.2",
		"meilisearch": "^0.35.0",
		"prom-client": "^14.2.0",
		"socket.io-client": "^4.7.2",
		"zod": "^3.22.2"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.7.2",
		"@commitlint/config-conventional": "^17.7.0",
		"@kbotdev/eslint-config": "^1.1.1",
		"@kbotdev/prettier-config": "^1.0.1",
		"@kbotdev/ts-config": "^1.0.1",
		"@swc/core": "^1.3.90",
		"@types/humanize-duration": "^3.27.1",
		"@types/node": "^20.7.2",
		"@typescript-eslint/eslint-plugin": "^6.7.3",
		"@typescript-eslint/parser": "^6.7.3",
		"@vitest/coverage-istanbul": "^0.34.6",
		"eslint": "8.50.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-import-resolver-typescript": "^3.6.1",
		"eslint-plugin-import": "^2.28.1",
		"eslint-plugin-prettier": "^5.0.0",
		"husky": "^8.0.3",
		"lint-staged": "^14.0.1",
		"prettier": "^3.0.3",
		"prisma": "^5.3.1",
		"tsup": "^7.2.0",
		"typescript": "^5.2.2",
		"vitepress": "^1.0.0-rc.20",
		"vitest": "^0.34.6"
	},
	"lint-staged": {
		"*.{mjs,cjs,js,ts}": [
			"eslint --fix --ext js,ts",
			"prettier --write --loglevel=warn \"**/*.{mjs,cjs,js,ts}\""
		],
		"*.{json,yml,yaml,md}": [
			"prettier --write --loglevel=warn \"**/*.{json,yml,yaml,md}\""
		],
		"*.prisma": [
			"prisma format"
		]
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"imports": {
		"#config": "./dist/config.js",
		"#lib/*": "./dist/lib/*.js",
		"#utils/*": "./dist/lib/utils/*.js"
	},
	"packageManager": "yarn@3.6.3"
}
